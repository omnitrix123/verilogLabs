:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55903b459d10 .scope module, "testbench" "testbench" 2 1;
 .timescale 0 0;
v0x55903b46cb30_0 .var *"_s0", 0 0; Local signal
v0x55903b46cc10_0 .var "clk1", 0 0;
v0x55903b46ccd0_0 .net "counter1", 5 0, v0x55903b46c820_0;  1 drivers
v0x55903b46cdd0_0 .var "reset1", 0 0;
S_0x55903b459e90 .scope module, "G" "question" 2 4, 3 1 0, S_0x55903b459d10;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /OUTPUT 6 "count"
    .port_info 2 /INPUT 1 "reset"
v0x55903b4220a0_0 .var "a", 5 0;
v0x55903b46c760_0 .net "clk", 0 0, v0x55903b46cc10_0;  1 drivers
v0x55903b46c820_0 .var "count", 5 0;
v0x55903b46c8e0_0 .net "reset", 0 0, v0x55903b46cdd0_0;  1 drivers
v0x55903b46c9a0_0 .var "var", 0 0;
E_0x55903b45a310/0 .event edge, v0x55903b46c8e0_0;
E_0x55903b45a310/1 .event posedge, v0x55903b46c760_0;
E_0x55903b45a310 .event/or E_0x55903b45a310/0, E_0x55903b45a310/1;
    .scope S_0x55903b459e90;
T_0 ;
    %pushi/vec4 62, 0, 6;
    %store/vec4 v0x55903b4220a0_0, 0, 6;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55903b46c9a0_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x55903b459e90;
T_1 ;
    %wait E_0x55903b45a310;
    %load/vec4 v0x55903b46c8e0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 32, 0, 6;
    %assign/vec4 v0x55903b46c820_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x55903b46c820_0;
    %cmpi/e 0, 0, 6;
    %jmp/0xz  T_1.2, 4;
    %pushi/vec4 3, 0, 6;
    %store/vec4 v0x55903b4220a0_0, 0, 6;
    %jmp T_1.3;
T_1.2 ;
    %load/vec4 v0x55903b46c820_0;
    %cmpi/e 27, 0, 6;
    %jmp/0xz  T_1.4, 4;
    %pushi/vec4 62, 0, 6;
    %store/vec4 v0x55903b4220a0_0, 0, 6;
    %jmp T_1.5;
T_1.4 ;
    %load/vec4 v0x55903b46c820_0;
    %pushi/vec4 9, 0, 6;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x55903b46c9a0_0;
    %pad/u 32;
    %pushi/vec4 1, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.6, 8;
    %pushi/vec4 0, 0, 6;
    %store/vec4 v0x55903b4220a0_0, 0, 6;
    %jmp T_1.7;
T_1.6 ;
    %load/vec4 v0x55903b46c820_0;
    %pushi/vec4 9, 0, 6;
    %cmp/e;
    %flag_get/vec4 4;
    %load/vec4 v0x55903b46c9a0_0;
    %pad/u 32;
    %pushi/vec4 0, 0, 32;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.8, 8;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55903b46c9a0_0, 0, 1;
T_1.8 ;
T_1.7 ;
T_1.5 ;
T_1.3 ;
    %load/vec4 v0x55903b46c820_0;
    %load/vec4 v0x55903b4220a0_0;
    %add;
    %assign/vec4 v0x55903b46c820_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55903b459d10;
T_2 ;
    %vpi_call 2 6 "$monitor", $time, "clk1=%b,reset1=%b,counter1=%b", v0x55903b46cc10_0, v0x55903b46cdd0_0, v0x55903b46ccd0_0 {0 0 0};
    %vpi_call 2 7 "$dumpfile", "waveform.vcd" {0 0 0};
    %vpi_call 2 8 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55903b459d10 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55903b46cc10_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55903b46cdd0_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55903b46cdd0_0, 0, 1;
    %delay 200, 0;
    %vpi_call 2 13 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x55903b459d10;
T_3 ;
    %load/vec4 v0x55903b46cc10_0;
    %inv;
    %store/vec4 v0x55903b46cb30_0, 0, 1;
    %pushi/vec4 2, 0, 64;
    %ix/vec4 4;
    %delayx 4;
    %load/vec4 v0x55903b46cb30_0;
    %store/vec4 v0x55903b46cc10_0, 0, 1;
    %jmp T_3;
    .thread T_3;
# The file index is used to find the file name in the following table.
