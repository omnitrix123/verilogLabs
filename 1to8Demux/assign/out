#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x5587ba0e0fc0 .scope module, "testbench" "testbench" 2 1;
 .timescale 0 0;
v0x5587ba116160_0 .var "a", 0 0;
v0x5587ba116230_0 .net "out", 7 0, v0x5587ba115f40_0;  1 drivers
v0x5587ba116300_0 .var "sel", 2 0;
S_0x5587ba0e1140 .scope module, "D1" "oneto8demux" 2 6, 3 1 0, S_0x5587ba0e0fc0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a"
    .port_info 1 /INPUT 3 "sel"
    .port_info 2 /OUTPUT 8 "out"
v0x5587ba1020d0_0 .net "a", 0 0, v0x5587ba116160_0;  1 drivers
v0x5587ba115f40_0 .var "out", 7 0;
v0x5587ba116020_0 .net "sel", 2 0, v0x5587ba116300_0;  1 drivers
E_0x5587ba102870 .event edge, v0x5587ba116020_0, v0x5587ba1020d0_0;
    .scope S_0x5587ba0e1140;
T_0 ;
    %wait E_0x5587ba102870;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x5587ba115f40_0, 0, 8;
    %load/vec4 v0x5587ba1020d0_0;
    %ix/getv 4, v0x5587ba116020_0;
    %store/vec4 v0x5587ba115f40_0, 4, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x5587ba0e0fc0;
T_1 ;
    %vpi_call 2 10 "$monitor", $time, "a=%b,sel=%b,out=%b", v0x5587ba116160_0, v0x5587ba116300_0, v0x5587ba116230_0 {0 0 0};
    %vpi_call 2 11 "$dumpfile", "waveform.vcd" {0 0 0};
    %vpi_call 2 12 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5587ba0e0fc0 {0 0 0};
    %delay 0, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5587ba116160_0, 0, 1;
    %pushi/vec4 1, 0, 3;
    %store/vec4 v0x5587ba116300_0, 0, 3;
    %delay 2, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5587ba116160_0, 0, 1;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x5587ba116300_0, 0, 3;
    %delay 4, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5587ba116160_0, 0, 1;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x5587ba116300_0, 0, 3;
    %delay 4, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5587ba116160_0, 0, 1;
    %pushi/vec4 1, 0, 3;
    %store/vec4 v0x5587ba116300_0, 0, 3;
    %delay 4, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5587ba116160_0, 0, 1;
    %pushi/vec4 2, 0, 3;
    %store/vec4 v0x5587ba116300_0, 0, 3;
    %delay 4, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5587ba116160_0, 0, 1;
    %pushi/vec4 3, 0, 3;
    %store/vec4 v0x5587ba116300_0, 0, 3;
    %delay 4, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5587ba116160_0, 0, 1;
    %pushi/vec4 4, 0, 3;
    %store/vec4 v0x5587ba116300_0, 0, 3;
    %delay 4, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5587ba116160_0, 0, 1;
    %pushi/vec4 5, 0, 3;
    %store/vec4 v0x5587ba116300_0, 0, 3;
    %delay 4, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5587ba116160_0, 0, 1;
    %pushi/vec4 6, 0, 3;
    %store/vec4 v0x5587ba116300_0, 0, 3;
