#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x563374ad2060 .scope module, "testbench" "testbench" 2 1;
 .timescale 0 0;
v0x563374b1b9e0_0 .var "en", 0 0;
v0x563374b1bab0_0 .var "in", 1 0;
v0x563374b1bb80_0 .net "out", 3 0, v0x563374b1b8a0_0;  1 drivers
S_0x563374ad21e0 .scope module, "t1" "twotofour" 2 6, 3 1 0, S_0x563374ad2060;
 .timescale 0 0;
    .port_info 0 /INPUT 2 "in"
    .port_info 1 /OUTPUT 4 "out"
    .port_info 2 /INPUT 1 "en"
v0x563374ae72b0_0 .net "en", 0 0, v0x563374b1b9e0_0;  1 drivers
v0x563374b1b7c0_0 .net "in", 1 0, v0x563374b1bab0_0;  1 drivers
v0x563374b1b8a0_0 .var "out", 3 0;
E_0x563374b07660 .event edge, v0x563374ae72b0_0, v0x563374b1b7c0_0;
    .scope S_0x563374ad21e0;
T_0 ;
    %wait E_0x563374b07660;
    %load/vec4 v0x563374ae72b0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x563374b1b7c0_0;
    %cmpi/e 0, 0, 2;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 1, 0, 4;
    %store/vec4 v0x563374b1b8a0_0, 0, 4;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x563374b1b7c0_0;
    %cmpi/e 1, 0, 2;
    %jmp/0xz  T_0.4, 4;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x563374b1b8a0_0, 0, 4;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v0x563374b1b7c0_0;
    %cmpi/e 2, 0, 2;
    %jmp/0xz  T_0.6, 4;
    %pushi/vec4 4, 0, 4;
    %store/vec4 v0x563374b1b8a0_0, 0, 4;
    %jmp T_0.7;
T_0.6 ;
    %load/vec4 v0x563374b1b7c0_0;
    %cmpi/e 3, 0, 2;
    %jmp/0xz  T_0.8, 4;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x563374b1b8a0_0, 0, 4;
    %jmp T_0.9;
T_0.8 ;
    %pushi/vec4 0, 15, 4;
    %store/vec4 v0x563374b1b8a0_0, 0, 4;
T_0.9 ;
T_0.7 ;
T_0.5 ;
T_0.3 ;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 0, 15, 4;
    %store/vec4 v0x563374b1b8a0_0, 0, 4;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x563374ad2060;
T_1 ;
    %vpi_call 2 10 "$dumpfile", "waveform.vcd" {0 0 0};
    %vpi_call 2 11 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x563374ad2060 {0 0 0};
    %vpi_call 2 12 "$monitor", $time, "in=%b,out=%b,en=%b", v0x563374b1bab0_0, v0x563374b1bb80_0, v0x563374b1b9e0_0 {0 0 0};
    %delay 0, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x563374b1b9e0_0, 0, 1;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x563374b1bab0_0, 0, 2;
    %delay 2, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x563374b1b9e0_0, 0, 1;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x563374b1bab0_0, 0, 2;
    %delay 4, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x563374b1b9e0_0, 0, 1;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x563374b1bab0_0, 0, 2;
    %delay 6, 0;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x563374b1bab0_0, 0, 2;
    %delay 8, 0;
    %pushi/vec4 0, 3, 2;
    %store/vec4 v0x563374b1bab0_0, 0, 2;
    %delay 10, 0;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
