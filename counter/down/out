#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55d98c01ae20 .scope module, "testbench" "testbench" 2 1;
 .timescale 0 0;
v0x55d98c02eae0_0 .var *"_s0", 0 0; Local signal
v0x55d98c02ebe0_0 .var "clk", 0 0;
v0x55d98c02eca0_0 .net "count", 4 0, v0x55d98c02e6c0_0;  1 drivers
v0x55d98c02eda0_0 .var "mode", 0 0;
v0x55d98c02ee70_0 .var "preset", 0 0;
v0x55d98c02ef60_0 .var "reset", 0 0;
S_0x55d98c01afa0 .scope module, "D1" "downcount" 2 5, 3 1 0, S_0x55d98c01ae20;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "reset"
    .port_info 2 /INPUT 1 "preset"
    .port_info 3 /INPUT 1 "mode"
    .port_info 4 /OUTPUT 5 "count"
v0x55d98bfe40d0_0 .net "clk", 0 0, v0x55d98c02ebe0_0;  1 drivers
v0x55d98c02e6c0_0 .var "count", 4 0;
v0x55d98c02e7a0_0 .net "mode", 0 0, v0x55d98c02eda0_0;  1 drivers
v0x55d98c02e870_0 .net "preset", 0 0, v0x55d98c02ee70_0;  1 drivers
v0x55d98c02e930_0 .net "reset", 0 0, v0x55d98c02ef60_0;  1 drivers
E_0x55d98c019990 .event posedge, v0x55d98bfe40d0_0;
    .scope S_0x55d98c01afa0;
T_0 ;
    %wait E_0x55d98c019990;
    %load/vec4 v0x55d98c02e930_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 5;
    %assign/vec4 v0x55d98c02e6c0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55d98c02e870_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %pushi/vec4 31, 0, 5;
    %assign/vec4 v0x55d98c02e6c0_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x55d98c02e7a0_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_0.4, 4;
    %load/vec4 v0x55d98c02e6c0_0;
    %addi 3, 0, 5;
    %assign/vec4 v0x55d98c02e6c0_0, 0;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v0x55d98c02e7a0_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.6, 4;
    %load/vec4 v0x55d98c02e6c0_0;
    %subi 2, 0, 5;
    %assign/vec4 v0x55d98c02e6c0_0, 0;
T_0.6 ;
T_0.5 ;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55d98c01ae20;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d98c02ebe0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x55d98c01ae20;
T_2 ;
    %load/vec4 v0x55d98c02ebe0_0;
    %inv;
    %store/vec4 v0x55d98c02eae0_0, 0, 1;
    %pushi/vec4 1, 0, 64;
    %ix/vec4 4;
    %delayx 4;
    %load/vec4 v0x55d98c02eae0_0;
    %store/vec4 v0x55d98c02ebe0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55d98c01ae20;
T_3 ;
    %vpi_call 2 12 "$monitor", $time, "clk=%b,reset=%b,preset=%b,mode=%b,count=%b", v0x55d98c02ebe0_0, v0x55d98c02ef60_0, v0x55d98c02ee70_0, v0x55d98c02eda0_0, v0x55d98c02eca0_0 {0 0 0};
    %vpi_call 2 13 "$dumpfile", "waveform.vcd" {0 0 0};
    %vpi_call 2 14 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55d98c01ae20 {0 0 0};
    %delay 0, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55d98c02ef60_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d98c02ef60_0, 0, 1;
    %delay 2, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55d98c02ee70_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d98c02eda0_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d98c02ee70_0, 0, 1;
    %delay 30, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55d98c02eda0_0, 0, 1;
    %delay 50, 0;
    %vpi_call 2 22 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
